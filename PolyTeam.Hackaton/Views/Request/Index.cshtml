@model PolyTeam.Hackaton.Models.SocialRequestModel
@using (Html.BeginForm("LogOn","Account")) {
    <div id="panel">
        <fieldset>
            <legend>Інформація користувача</legend>

            <div class="editor-label" class="field">
                @Html.LabelFor(m => m.Street)
                @Html.TextBoxFor(m => m.Street)
            </div>
            <div class="editor-field" class="field">
                @Html.LabelFor(m => m.House)
                @Html.TextBoxFor(m => m.House)
            </div>
            <div class="editor-field" class="field">
                @Html.LabelFor(m => m.Flat)
                @Html.TextBoxFor(m => m.Flat)
            </div>
            <div class='SubmitButton'>
                <input type="submit" value="Додати" />
            </div>
        </fieldset>
    </div>
}